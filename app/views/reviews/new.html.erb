<h1>Crear review</h1>

<%= form_with(model: @review, method: :post) do |f| %>
    <div class="field">
        <%= f.label :calificacion %>
        <%= f.text_field :calificacion %>
    </div>

    <div class="field">
        <%= f.label :comentario %>
        <%= f.text_area :comentario %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>

<%= link_to "Volver al listado de reviews", reviews_index_path %>
